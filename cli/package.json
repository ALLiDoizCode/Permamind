{
  "name": "@permamind/skills-cli",
  "version": "2.1.4",
  "type": "module",
  "description": "CLI tool for publishing, searching, and installing Claude Agent Skills",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./lib/*": "./dist/lib/*.js",
    "./types/*": "./dist/types/*.js"
  },
  "typesVersions": {
    "*": {
      "lib/*": [
        "dist/lib/*"
      ],
      "types/*": [
        "dist/types/*"
      ]
    }
  },
  "bin": {
    "skills": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --watch"
  },
  "dependencies": {
    "@ardrive/turbo-sdk": "^1.35.0",
    "@permaweb/aoconnect": "^0.0.53",
    "ajv": "^8.12.0",
    "arweave": "^1.14.4",
    "bip39": "^3.1.0",
    "bip39-web-crypto": "^4.0.1",
    "chalk": "5.6.2",
    "cli-table3": "^0.6.3",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "gray-matter": "^4.0.3",
    "human-crypto-keys": "^0.1.4",
    "keytar": "^7.9.0",
    "lodash": "^4.17.21",
    "node-arweave-wallet": "^0.0.12",
    "ora": "5.4.1",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/bip39": "^2.4.2",
    "@types/tar": "^6.1.11",
    "ts-jest": "^29.4.5"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "keywords": [
    "claude",
    "agent-skills",
    "cli",
    "arweave",
    "ao",
    "decentralized",
    "registry"
  ],
  "author": "Permamind Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ALLiDoizCode/Permamind.git",
    "directory": "cli"
  },
  "homepage": "https://github.com/ALLiDoizCode/Permamind#readme",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
}
