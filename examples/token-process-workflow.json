{
  "id": "ao-token-standard-v1",
  "name": "AO Standard Token",
  "description": "A comprehensive AO token implementation with transfer, balance, allowance, and metadata capabilities following AO token standards",
  "version": "1.2.0",
  "author": "AO Community",
  "processId": "YOUR_TOKEN_PROCESS_ID_HERE",
  "capabilities": [
    "token-transfer",
    "balance-query", 
    "allowance-management",
    "token-metadata",
    "mint-burn",
    "ownership-control"
  ],
  "category": "finance",
  "tags": ["token", "currency", "finance", "erc20-like", "fungible"],
  "network": "ao",
  "scheduler": "TZ7o7SIZ06ZpJrjHvjKf2kOVV_2tpKUYk9_9SFlyUO0",
  "module": "cbn0KKrBZH7hdNkNokuXLtGryrWM--PjSTBqIzw9Kkk",
  "handlers": [
    {
      "name": "Transfer",
      "description": "Transfer tokens from sender to recipient",
      "messageSchema": {
        "action": "Transfer",
        "description": "Send tokens to another address",
        "tags": [
          {
            "name": "Action",
            "value": "Transfer",
            "required": true,
            "description": "The action to perform"
          },
          {
            "name": "Recipient",
            "value": "",
            "required": true,
            "description": "The address to send tokens to",
            "examples": ["1234567890abcdef1234567890abcdef12345678", "ao-process-id-here"]
          },
          {
            "name": "Quantity",
            "value": "",
            "required": true,
            "description": "Amount of tokens to transfer (in smallest units)",
            "examples": ["100", "1000000", "50000000000"]
          }
        ],
        "examples": [
          {
            "description": "Transfer 100 tokens",
            "tags": [
              { "name": "Action", "value": "Transfer" },
              { "name": "Recipient", "value": "abc123def456ghi789jkl012mno345pqr678stu90" },
              { "name": "Quantity", "value": "100" }
            ],
            "expectedResponse": "Transfer successful - 100 tokens sent to abc123...stu90"
          },
          {
            "description": "Transfer with large amount",
            "tags": [
              { "name": "Action", "value": "Transfer" },
              { "name": "Recipient", "value": "xyz987uvw654rst321nop098mno765abc432def10" },
              { "name": "Quantity", "value": "1000000000000" }
            ],
            "expectedResponse": "Transfer successful"
          }
        ]
      },
      "responsePatterns": [
        {
          "messageType": "success",
          "indicators": {
            "tags": [{ "name": "Status", "values": ["Success"] }]
          },
          "format": {
            "structured": false,
            "dataType": "string",
            "parser": "Success message with transfer details"
          }
        },
        {
          "messageType": "error",
          "indicators": {
            "errorCodes": ["Insufficient-Balance", "Invalid-Recipient", "Transfer-Failed"]
          },
          "format": {
            "structured": false,
            "dataType": "string"
          }
        }
      ],
      "capabilities": ["token-transfer"],
      "rateLimit": {
        "requestsPerMinute": 100
      },
      "costs": {
        "computeUnits": 50,
        "description": "Network processing fee"
      }
    },
    {
      "name": "Balance",
      "description": "Check token balance for any address",
      "messageSchema": {
        "action": "Balance",
        "description": "Query the token balance of an address",
        "tags": [
          {
            "name": "Action",
            "value": "Balance",
            "required": true,
            "description": "The action to perform"
          },
          {
            "name": "Target",
            "value": "",
            "required": false,
            "description": "Address to check balance for (defaults to sender if omitted)",
            "examples": ["1234567890abcdef1234567890abcdef12345678", "your-address-here"]
          }
        ],
        "examples": [
          {
            "description": "Check your own balance",
            "tags": [
              { "name": "Action", "value": "Balance" }
            ],
            "expectedResponse": "Balance: 1500"
          },
          {
            "description": "Check another address balance",
            "tags": [
              { "name": "Action", "value": "Balance" },
              { "name": "Target", "value": "abc123def456ghi789jkl012mno345pqr678stu90" }
            ],
            "expectedResponse": "Balance: 750"
          }
        ]
      },
      "responsePatterns": [
        {
          "messageType": "data",
          "indicators": {},
          "format": {
            "structured": false,
            "dataType": "string",
            "parser": "Extract numeric balance from 'Balance: X' format"
          }
        }
      ],
      "capabilities": ["balance-query"],
      "rateLimit": {
        "requestsPerMinute": 500
      }
    },
    {
      "name": "Info",
      "description": "Get comprehensive token information and metadata",
      "messageSchema": {
        "action": "Info",
        "description": "Retrieve detailed token metadata",
        "tags": [
          {
            "name": "Action",
            "value": "Info",
            "required": true,
            "description": "The action to perform"
          }
        ],
        "examples": [
          {
            "description": "Get token information",
            "tags": [
              { "name": "Action", "value": "Info" }
            ],
            "expectedResponse": "JSON object with name, symbol, decimals, totalSupply, etc."
          }
        ]
      },
      "responsePatterns": [
        {
          "messageType": "data",
          "indicators": {},
          "format": {
            "structured": true,
            "dataType": "json",
            "parser": "Parse JSON response with token metadata"
          }
        }
      ],
      "capabilities": ["token-metadata"],
      "rateLimit": {
        "requestsPerMinute": 1000
      }
    },
    {
      "name": "Mint",
      "description": "Mint new tokens (owner only)",
      "messageSchema": {
        "action": "Mint",
        "description": "Create new tokens and assign to an address",
        "tags": [
          {
            "name": "Action",
            "value": "Mint",
            "required": true,
            "description": "The action to perform"
          },
          {
            "name": "Quantity",
            "value": "",
            "required": true,
            "description": "Amount of tokens to mint",
            "examples": ["1000", "1000000000000"]
          },
          {
            "name": "Target",
            "value": "",
            "required": false,
            "description": "Address to receive minted tokens (defaults to sender)",
            "examples": ["recipient-address-here"]
          }
        ],
        "examples": [
          {
            "description": "Mint 1000 tokens to yourself",
            "tags": [
              { "name": "Action", "value": "Mint" },
              { "name": "Quantity", "value": "1000" }
            ],
            "expectedResponse": "Minted 1000 tokens"
          },
          {
            "description": "Mint tokens to specific address",
            "tags": [
              { "name": "Action", "value": "Mint" },
              { "name": "Quantity", "value": "5000" },
              { "name": "Target", "value": "recipient-address-here" }
            ],
            "expectedResponse": "Minted 5000 tokens to recipient-address-here"
          }
        ]
      },
      "responsePatterns": [
        {
          "messageType": "success",
          "indicators": {},
          "format": {
            "structured": false,
            "dataType": "string"
          }
        },
        {
          "messageType": "error",
          "indicators": {
            "errorCodes": ["Unauthorized", "Invalid-Quantity", "Mint-Failed"]
          },
          "format": {
            "structured": false,
            "dataType": "string"
          }
        }
      ],
      "capabilities": ["mint-burn", "ownership-control"],
      "requirements": ["owner-privileges"],
      "rateLimit": {
        "requestsPerMinute": 10
      }
    },
    {
      "name": "Burn",
      "description": "Burn (destroy) tokens from sender's balance",
      "messageSchema": {
        "action": "Burn",
        "description": "Destroy tokens from your balance",
        "tags": [
          {
            "name": "Action",
            "value": "Burn",
            "required": true,
