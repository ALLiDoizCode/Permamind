          "format": {
            "structured": false,
            "dataType": "string"
          }
        }
      ],
      "capabilities": ["mint-burn"],
      "rateLimit": {
        "requestsPerMinute": 50
      }
    },
    {
      "name": "Approve",
      "description": "Approve another address to spend tokens on your behalf",
      "messageSchema": {
        "action": "Approve",
        "description": "Grant spending allowance to another address",
        "tags": [
          {
            "name": "Action",
            "value": "Approve",
            "required": true,
